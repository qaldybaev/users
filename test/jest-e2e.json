{
    "preset": "ts-jest",
    "testEnvironment": "node",
    "transform": {
        "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": ".*\\.e2e-spec\\.ts$",
    "moduleFileExtensions": [
        "ts",
        "js",
        "json"
    ],
    "rootDir": "../",
    "collectCoverage": false,
    "coverageDirectory": "./coverage",
    "maxWorkers": 2,
    "testTimeout": 30000
}